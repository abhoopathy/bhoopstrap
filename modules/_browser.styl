.{q}-browser
    border-radius 9px
    background rgba(212,212,212,0.25)
    border 1px solid rgba(150,150,150,0.18)
    box-shadow 0px 7px 12px 0px rgba(0,0,0,0.15)
    width 938px
    1px solid rgb(197, 197, 197)

    .{q}-browser-toolbar
        padding 5px
        border-bottom 1px solid rgba(150,150,150,6)
        clearfix()

        .{q}-float-l, .{q}-float-r
            display inline-block

        .{q}-browser-circle-button
            display inline-block
            width 10px
            height 10px
            border-radius 5px
            margin-left 5px
            background #D8D8D8
            border 1px solid #979797

    .{q}-browser-contents
        width 100%
        background grey
